<%- include("../index.ejs") %>

<h2><%= title %></h2>
<section class="category-section">
  <div class="add-button-container"><a href="/artists/create" class="add-button">Add</a></div>
<ul>
  <% if(artists.length > 0 ) { %>
    <% for(const artist of artists) {%>
        <li><a href="<%= artist.url%>"><%= artist.name%></a></li>
    <% } %>
  <% } else { %>
    <p>There's no artists yet.</p>
    <% } %>
</ul>
</section>