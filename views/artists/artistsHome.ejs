
<div class="sticky-header">
  <%- include("../index.ejs") %>
  <h2><%= title %></h2>
  <div class="add-button-container add-element"><a href="/artists/create" class="add-button">Add</a></div>
</div>

<section class="category-section">
<ul>
  <% if(artists.length > 0 ) { %>
    <% for(const artist of artists) {%>
        <li><a href="<%= artist.url%>"><%= artist.name%></a></li>
    <% } %>
  <% } else { %>
    <p>There's no artists yet.</p>
    <% } %>
</ul>
</section>
<%- include('../partials/footer.ejs') %>