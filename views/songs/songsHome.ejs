
<div class="sticky-header">
  <%- include("../index.ejs") %>
  <h2><%= title %></h2>
  <div class="add-button-container add-element"><a href="/songs/create" class="add-button">Add</a></div>
</div>
<section class="category-section songs-section">
<ul>
  <% if(songs.length > 0 ) { %>
    <% for(const song of songs) {%>
        <li><a href="<%= song.url%>"><%= song.title%></a> by <a href="<%= song.artist.url %>"><%= song.artist.name%></a></li>
    <% } %>
  <% } else { %>
    <p>There's no songs yet.</p>
    <% } %>
</ul>
</section>
<%- include('../partials/footer.ejs') %>